{
    "manifestVersion":  1,
    "id":  "FitnesseRun",
    "name":  "FitNesseRun",
    "version":  "0.4.43",
    "publisher":  "rikessenius",
    "targets":  [
                    {
                        "id":  "Microsoft.VisualStudio.Services"
                    }
                ],
    "description":  "Tasks to run a FitNesse test and save the result in NUnit3 format, and to configure FitNesse.",
    "categories":  [
                       "Azure Pipelines"
                   ],
    "tags":  [
                 "FitNesse",
                 "Test",
                 "Test automation",
                 "Test task",
                 "Testing"
             ],
    "links":  {
                  "repository":  {
                                     "uri":  "https://github.com/essenius/FitNesseRun"
                                 },
                  "issues":  {
                                 "uri":  "https://github.com/essenius/FitNesseRun/issues"
                             }
              },
    "repository":  {
                       "type":  "git",
                       "uri":  "https://github.com/essenius/FitNesseRun"
                   },
    "icons":  {
                  "default":  "fitnesse-logo.png"
              },
    "content":  {
                    "details":  {
                                    "path":  "readme.md"
                                },
                    "license":  {
                                    "path":  "license.md"
                                }
                },
    "screenshots":  [
                        {
                            "path":  "screenshot-define.jpg"
                        },
                        {
                            "path":  "screenshot-testresult.jpg"
                        }
                    ],
    "files":  [
                  {
                      "path":  "Common/CommonFunctions.psm1",
                      "packagePath":  "FitNesseRun/FitNesseRunV0/CommonFunctions.psm1"
                  },
                  {
                      "path":  "Common/ps_modules",
                      "packagePath":  "FitNesseRun/FitNesseRunV0/ps_modules"
                  },
                  {
                      "path":  "Common/CommonFunctions.psm1",
                      "packagePath":  "FitNesseConfigure/FitNesseConfigureV0/CommonFunctions.psm1"
                  },
                  {
                      "path":  "Common/ps_modules",
                      "packagePath":  "FitNesseConfigure/FitNesseConfigureV0/ps_modules"
                  },
                  {
                      "path":  "FitNesseRun/FitNesseRunV0/FitNesseRun.ps1"
                  },
                  {
                      "path":  "FitNesseRun/FitNesseRunV0/FitNesseToDetailedResults.xslt"
                  },
                  {
                      "path":  "FitNesseRun/FitNesseRunV0/FitNesseToNUNit3.xslt"
                  },
                  {
                      "path":  "FitNesseRun/FitNesseRunV0/FitNesseToSummaryResult.xslt"
                  },
                  {
                      "path":  "FitNesseRun/FitNesseRunV0/icon.png"
                  },
                  {
                      "path":  "FitNesseRun/FitNesseRunV0/task.json"
                  },
                  {
                      "path":  "FitNesseConfigure/FitNesseConfigureV0/FitNesseConfigure.ps1"
                  },
                  {
                      "path":  "FitNesseConfigure/FitNesseConfigureV0/icon.png"
                  },
                  {
                      "path":  "FitNesseConfigure/FitNesseConfigureV0/task.json"
                  },
                  {
                      "path":  "Screenshot-Parameters.png",
                      "addressable":  "true"
                  },
                  {
                      "path":  "fitnesse-logo.png",
                      "addressable":  "true"
                  }
              ],
    "contributions":  [
                          {
                              "id":  "fitnesse-run-task",
                              "type":  "ms.vss-distributed-task.task",
                              "targets":  [
                                              "ms.vss-distributed-task.tasks"
                                          ],
                              "properties":  {
                                                 "name":  "FitNesseRun"
                                             }
                          },
                          {
                              "id":  "fitnesse-configure-task",
                              "type":  "ms.vss-distributed-task.task",
                              "targets":  [
                                              "ms.vss-distributed-task.tasks"
                                          ],
                              "properties":  {
                                                 "name":  "FitNesseConfigure"
                                             }
                          }
                      ]
}

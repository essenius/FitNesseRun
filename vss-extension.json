{
    "manifestVersion":  1,
    "id":  "FitnesseRun",
    "name":  "FitNesseRun",
    "version":  "0.4.34",
    "publisher":  "rikessenius",
    "targets":  [
                    {
                        "id":  "Microsoft.VisualStudio.Services"
                    }
                ],
    "description":  "Tasks to run a FitNesse test and save the result in NUnit3 format, and to configure FitNesse.",
    "categories":  [
                       "Build and release"
                   ],
    "icons":  {
                  "default":  "fitnesse-logo.png"
              },
    "content":  {
                    "details":  {
                                    "path":  "readme.md"
                                },
                    "license":  {
                                    "path":  "license.md"
                                }
                },
    "screenshots":  [
                        {
                            "path":  "screenshot-define.jpg"
                        },
                        {
                            "path":  "screenshot-testresult.jpg"
                        }
                    ],
    "files":  [
                  {
                      "path":  "Common/CommonFunctions.psm1",
                      "packagePath":  "FitNesseRun/CommonFunctions.psm1"
                  },
                  {
                      "path":  "Common/ps_modules",
                      "packagePath":  "FitNesseRun/ps_modules"
                  },
                  {
                      "path":  "Common/CommonFunctions.psm1",
                      "packagePath":  "FitNesseConfigure/CommonFunctions.psm1"
                  },
                  {
                      "path":  "Common/ps_modules",
                      "packagePath":  "FitNesseConfigure/ps_modules"
                  },
                  {
                      "path":  "FitNesseRun/FitNesseRun.ps1"
                  },
                  {
                      "path":  "FitNesseRun/FitNesseToDetailedResults.xslt"
                  },
                  {
                      "path":  "FitNesseRun/FitNesseToNUNit3.xslt"
                  },
                  {
                      "path":  "FitNesseRun/FitNesseToSummaryResult.xslt"
                  },
                  {
                      "path":  "FitNesseRun/icon.png"
                  },
                  {
                      "path":  "FitNesseRun/task.json"
                  },
                  {
                      "path":  "FitNesseConfigure/FitNesseConfigure.ps1"
                  },
                  {
                      "path":  "FitNesseConfigure/icon.png"
                  },
                  {
                      "path":  "FitNesseConfigure/task.json"
                  },
                  {
                      "path":  "Screenshot-Parameters.png",
                      "addressable":  "true"
                  },
                  {
                      "path":  "fitnesse-logo.png",
                      "addressable":  "true"
                  }
              ],
    "contributions":  [
                          {
                              "id":  "fitnesse-run-task",
                              "type":  "ms.vss-distributed-task.task",
                              "targets":  [
                                              "ms.vss-distributed-task.tasks"
                                          ],
                              "properties":  {
                                                 "name":  "FitNesseRun"
                                             }
                          },
                          {
                              "id":  "fitnesse-configure-task",
                              "type":  "ms.vss-distributed-task.task",
                              "targets":  [
                                              "ms.vss-distributed-task.tasks"
                                          ],
                              "properties":  {
                                                 "name":  "FitNesseConfigure"
                                             }
                          }
                      ]
}
